{
    "gpt-5-nano-2025-08-07": {
        "comment": "Primary OpenAI GPT-5 Nano model (Responses API, streaming + reasoning)",
        "priority": 1,
        "provider": "openai",
        "model": "gpt-5-nano-2025-08-07",
        "cost": {
            "input": 0.05,
            "output": 0.40
        }
    },

    "gpt-5-mini-2025-08-07": {
        "comment": "Secondary OpenAI GPT-5 Mini model (Responses API, streaming + reasoning)",
        "priority": 2,
        "provider": "openai",
        "model": "gpt-5-mini-2025-08-07",
        "cost": {
            "input": 0.25,
            "output": 2.00
        }
    },

    "gpt-4o-mini-2024-07-18": {
        "comment": "Ancient OpenAI model. 128K context. No reasoning, emergency fallback only",
        "priority": 3,
        "provider": "openai",
        "model": "gpt-4o-mini-2024-07-18",
        "cost": {
            "input": 0.15,
            "output": 0.60
        },
        "arguments": {
            "api_key": "${OPENAI_API_KEY}",    
            "is_function_calling_model": true,
         
            "max_retries": 3
        }
    },

    "gpt-image-1-mini": {
        "comment": "Standard OpenAI image generation model for all image tasks",
        "priority": 90,
        "provider": "openai",
        "model": "gpt-image-1-mini",
        "capabilities": ["image_generation"],
        "image_defaults": {
            "default_size": "1024x1024",
            "allowed_sizes": [
                "1024x1024",
                "1024x1536",
                "1536x1024",
                "1024x1792",
                "1792x1024"
            ],
            "quality": "low",
            "allowed_qualities": [
                "low",
                "medium",
                "high",
                "auto"
            ],
            "default_output_format": "png",
            "allowed_output_formats": [
                "png",
                "jpeg",
                "webp"
            ],
            "default_output_compression": 85,
            "timeout_seconds": 60,
            "max_retries": 2,
            "prompt_style_suffix": "professional business concept art style, clean and modern aesthetic, suitable for business presentations and planning documents"
        }
    }
}
