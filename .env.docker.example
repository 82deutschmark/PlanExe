# PlanExe Docker Environment Configuration
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_PASSWORD=your_secure_database_password_here
DATABASE_URL=postgresql://planexe_user:your_secure_database_password_here@localhost:5432/planexe

# LLM API Keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Custom paths
PLANEXE_RUN_DIR=/tmp/planexe_runs
PATH_TO_PYTHON=/usr/local/bin/python

# UI Configuration (for production deployment)
PLANEXE_API_URL=http://localhost:8000

# Security (for multi-user deployments)
JWT_SECRET_KEY=your_jwt_secret_key_here